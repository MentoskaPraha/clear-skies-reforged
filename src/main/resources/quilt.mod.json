{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.mentoskapraha",
		"id": "quilted_clear_skies",
		"version": "${version}",
		"metadata": {
			"name": "Quilted Clear Skies",
			"description": "An updated fork of the Clear Skies mod adapted for the Quilt Mod-loader. This is a tiny mod that removes the banding at the horizon of Vanilla Minecraft.",
			"contributors": {
				"MentoskaPraha": "Owner"
			},
			"icon": "assets/icon.png"
		},
        "intermediate_mappings": "net.fabricmc:intermediary",
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=${loader_version}-"
			},
			{
				"id": "minecraft",
				"versions": "=${minecraft_version}"
			}
		]
	},
	"mixin": "quilted_clear_skies.mixins.json"
}